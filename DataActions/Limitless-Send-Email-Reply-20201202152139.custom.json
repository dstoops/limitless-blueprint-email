{
  "name": "Limitless Send Email Reply - Exported 2020-12-02 @ 15:21",
  "integrationType": "custom-rest-actions",
  "actionType": "custom",
  "config": {
    "request": {
      "requestUrlTemplate": "https://inject.socketlabs.com/api/v1/email",
      "requestType": "POST",
      "headers": {
        "Content-Type": "application/json"
      },
      "requestTemplate": "{\n  \"serverId\": \"32298\",\n  \"APIKey\": \"Xw42Hmr9CMb8j6D5Zea3\",\n  \"Messages\": [\n    {\n      \"To\": [\n        {\n          \"emailAddress\": \"$esc.jsonString(${input.ToAddress})\"\n        }\n      ],\n      \"From\": {\n        \"emailAddress\": \"experts@comfortgenie.mypurecloud.com\",\n        \"friendlyName\": \"Expert Support\"\n      },\n      \"ReplyTo\": {\n        \"emailAddress\": \"experts@comfortgenie.mypurecloud.com\",\n        \"friendlyName\": \"Expert Support\"\n      },\n      \"Subject\": \"$esc.jsonString(${input.Subject})\",\n      \"HtmlBody\": \"$esc.jsonString(${input.Body})\"\n    }\n  ]\n}"
    },
    "response": {
      "translationMap": {},
      "translationMapDefaults": {},
      "successTemplate": "${rawResult}"
    }
  },
  "contract": {
    "input": {
      "inputSchema": {
        "title": "message",
        "type": "object",
        "properties": {
          "Subject": {
            "type": "string"
          },
          "Body": {
            "type": "string"
          },
          "ToAddress": {
            "type": "string"
          },
          "ToName": {
            "type": "string"
          }
        },
        "additionalProperties": true
      }
    },
    "output": {
      "successSchema": {
        "type": "object",
        "properties": {},
        "additionalProperties": true
      }
    }
  },
  "secure": false
}